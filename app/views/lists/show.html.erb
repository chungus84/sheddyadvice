<div class="container">
  <h1><%= @list.name %></h1>
  <% @list.listposts.each do |listpost| %>
    <p><%= listpost.post.title %></p>
  <% end %>

  <h1>Add a Post to your List </h1>
  <%= simple_form_for([@list, @listpost]) do |f| %>
        <%= f.input :post_id %>
        <%= f.submit class: "button-primary" %>
  <% end %>

</div>
